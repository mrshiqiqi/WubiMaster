<UserControl x:Class="WubiMaster.Controls.EtymonKeyControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             d:DesignHeight="450"
             d:DesignWidth="800"
             Background="{DynamicResource bg-100}"
             mc:Ignorable="d">

    <UserControl.Resources>

        <BooleanToVisibilityConverter x:Key="BooleanToVisibilityConverter" />

        <Style x:Key="LeftStackPanelStyle" TargetType="StackPanel">
            <Setter Property="Orientation" Value="Horizontal" />
            <Setter Property="HorizontalAlignment" Value="Left" />
        </Style>

        <Style x:Key="RightStackPanelStyle" TargetType="StackPanel">
            <Setter Property="Orientation" Value="Horizontal" />
            <Setter Property="HorizontalAlignment" Value="Right" />
        </Style>

        <Style x:Key="CenterStackPanelStyle" TargetType="StackPanel">
            <Setter Property="Orientation" Value="Horizontal" />
            <Setter Property="HorizontalAlignment" Value="Center" />
        </Style>

        <Style x:Key="KeyBorderStyle"
               BasedOn="{StaticResource UIElementHover}"
               TargetType="Border">
            <Setter Property="Margin" Value="1,5" />
            <Setter Property="Padding" Value="3,2" />
            <Setter Property="Background" Value="{DynamicResource bg-200}" />
            <Setter Property="CornerRadius" Value="10" />
            <Setter Property="Width" Value="70" />
            <Setter Property="Height" Value="75" />
            <Setter Property="BorderThickness" Value="1" />
            <Setter Property="BorderBrush" Value="{DynamicResource bg-300}" />
        </Style>

        <Style x:Key="FirstKeyTextStyle" TargetType="TextBlock">
            <Setter Property="FontSize" Value="14" />
            <Setter Property="FontWeight" Value="Bold" />
            <Setter Property="VerticalAlignment" Value="Center" />
            <Setter Property="HorizontalAlignment" Value="Center" />
            <Setter Property="Foreground" Value="{DynamicResource primary-100}" />
            <Setter Property="FontFamily" Value="../Fonts/#黑体字根" />
        </Style>

        <Style x:Key="PrimaryKeyTextStyle" TargetType="TextBlock">
            <Setter Property="FontSize" Value="12" />
            <Setter Property="FontWeight" Value="Normal" />
            <Setter Property="VerticalAlignment" Value="Center" />
            <Setter Property="HorizontalAlignment" Value="Center" />
            <Setter Property="Foreground" Value="{DynamicResource text-100}" />
            <Setter Property="FontFamily" Value="../Fonts/#黑体字根" />
        </Style>

        <Style x:Key="SubKeyTextStyle" TargetType="TextBlock">
            <Setter Property="FontSize" Value="9" />
            <Setter Property="FontWeight" Value="Normal" />
            <Setter Property="VerticalAlignment" Value="Center" />
            <Setter Property="HorizontalAlignment" Value="Center" />
            <Setter Property="Foreground" Value="{DynamicResource text-200}" />
            <Setter Property="FontFamily" Value="../Fonts/#黑体字根" />
            <Setter Property="TextOptions.TextRenderingMode" Value="ClearType" />
        </Style>

        <Style x:Key="FanKeyTextStyle"
               BasedOn="{StaticResource SubKeyTextStyle}"
               TargetType="TextBlock">
            <Setter Property="Foreground" Value="{DynamicResource primary-200}" />
            <Setter Property="Visibility" Value="{Binding RelativeSource={RelativeSource Mode=FindAncestor, AncestorType={x:Type UserControl}}, Path=IsShowFanKey, Converter={StaticResource BooleanToVisibilityConverter}}" />
        </Style>

        <Style x:Key="StructureKeyTextStyle" TargetType="TextBlock">
            <Setter Property="FontSize" Value="14" />
            <Setter Property="FontWeight" Value="Bold" />
            <Setter Property="VerticalAlignment" Value="Center" />
            <Setter Property="HorizontalAlignment" Value="Center" />
            <Setter Property="Foreground" Value="{DynamicResource accent-100}" />
            <Setter Property="FontFamily" Value="../Fonts/#黑体字根" />
        </Style>

        <Style x:Key="NumberKeyTextStyle" TargetType="TextBlock">
            <Setter Property="FontSize" Value="12" />
            <Setter Property="FontWeight" Value="Bold" />
            <Setter Property="VerticalAlignment" Value="Center" />
            <Setter Property="HorizontalAlignment" Value="Right" />
            <Setter Property="Foreground" Value="{DynamicResource accent-200}" />
            <Setter Property="Margin" Value="0,0,2,0" />
        </Style>

        <Style x:Key="NameKeyTextStyle" TargetType="TextBlock">
            <Setter Property="FontSize" Value="12" />
            <Setter Property="FontWeight" Value="Bold" />
            <Setter Property="VerticalAlignment" Value="Center" />
            <Setter Property="HorizontalAlignment" Value="Left" />
            <Setter Property="Foreground" Value="{DynamicResource accent-200}" />
            <Setter Property="Margin" Value="2,0,0,0" />
        </Style>

        <Style x:Key="FormulaTitleStyle" TargetType="TextBlock">
            <Setter Property="HorizontalAlignment" Value="Center" />
            <Setter Property="VerticalAlignment" Value="Top" />
            <Setter Property="FontFamily" Value="../Fonts/#黑体字根" />
            <Setter Property="FontSize" Value="22" />
            <Setter Property="FontWeight" Value="Normal" />
            <Setter Property="Foreground" Value="{DynamicResource accent-100}" />
            <Setter Property="Margin" Value="0,0,0,5" />
        </Style>

        <Style x:Key="FormulaNumberStyle" TargetType="TextBlock">
            <Setter Property="HorizontalAlignment" Value="Left" />
            <Setter Property="VerticalAlignment" Value="Top" />
            <Setter Property="FontSize" Value="20" />
            <Setter Property="FontWeight" Value="Light" />
            <Setter Property="Foreground" Value="{DynamicResource accent-200}" />
            <Setter Property="Margin" Value="5,0" />
        </Style>

        <Style x:Key="FormulaTextStyle" TargetType="TextBlock">
            <Setter Property="HorizontalAlignment" Value="Right" />
            <Setter Property="VerticalAlignment" Value="Top" />
            <Setter Property="FontSize" Value="20" />
            <Setter Property="FontWeight" Value="Normal" />
            <Setter Property="Foreground" Value="{DynamicResource text-100}" />
            <Setter Property="TextWrapping" Value="Wrap" />
        </Style>

        <Style x:Key="FormulaGridStyle" TargetType="Grid">
            <Setter Property="VerticalAlignment" Value="Top" />
            <Setter Property="HorizontalAlignment" Value="Center" />
            <Setter Property="Margin" Value="10,0" />
        </Style>
    </UserControl.Resources>

