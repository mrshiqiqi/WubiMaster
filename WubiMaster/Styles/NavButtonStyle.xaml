<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:c="clr-namespace:WubiMaster.Controls">

    <Style x:Key="NavButtonStyle" TargetType="c:NavButton">
        <Setter Property="Height" Value="45" />
        <Setter Property="IconColor" Value="{DynamicResource bg-300}" />
        <Setter Property="IcontFontSize" Value="24" />
        <Setter Property="TextColor" Value="{DynamicResource text-200}" />
        <Setter Property="TextFontSize" Value="15" />
    </Style>
</ResourceDictionary>