<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <!--
        primary-100:#8B5FBF;
        primary-200:#61398F;
        primary-300:#FFFFFF;
        accent-100:#D6C6E1;
        accent-200:#9A73B5;
        text-100:#4A4A4A;
        text-200:#878787;
        bg-100:#F5F3F7;
        bg-200:#E9E4ED;
        bg-300:#FFFFFF;
    -->

    <SolidColorBrush x:Key="primary-100" Color="#8B5FBF" />
    <SolidColorBrush x:Key="primary-200" Color="#61398F" />
    <SolidColorBrush x:Key="primary-300" Color="#FFFFFF" />
    <SolidColorBrush x:Key="accent-100" Color="#D6C6E1" />
    <SolidColorBrush x:Key="accent-200" Color="#9A73B5" />
    <SolidColorBrush x:Key="text-100" Color="#4A4A4A" />
    <SolidColorBrush x:Key="text-200" Color="#878787" />
    <SolidColorBrush x:Key="bg-100" Color="#F5F3F7" />
    <SolidColorBrush x:Key="bg-200" Color="#E9E4ED" />
    <SolidColorBrush x:Key="bg-300" Color="#FFFFFF" />

    <Color x:Key="primary-1000">#8B5FBF</Color>
    <Color x:Key="primary-2000">#61398F</Color>
    <Color x:Key="primary-3000">#FFFFFF</Color>
    <Color x:Key="accent-1000">#D6C6E1</Color>
    <Color x:Key="accent-2000">#9A73B5</Color>
    <Color x:Key="text-1000">#4A4A4A</Color>
    <Color x:Key="text-2000">#878787</Color>
    <Color x:Key="bg-1000">#F5F3F7</Color>
    <Color x:Key="bg-2000">#E9E4ED</Color>
    <Color x:Key="bg-3000">#FFFFFF</Color>
</ResourceDictionary>