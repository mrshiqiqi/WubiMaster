<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <!--
        primary-100:#00A8E8;
        primary-200:#0076A2;
        primary-300:#BAECFF;
        accent-100:#FFB400;
        accent-200:#B37E00;
        text-100:#FFFFFF;
        text-200:#cbcbcb;
        bg-100:#007BA7;
        bg-200:#00759F;
        bg-300:#00A0D9;
    -->

    <SolidColorBrush x:Key="primary-100" Color="#00A8E8" />
    <SolidColorBrush x:Key="primary-200" Color="#0076A2" />
    <SolidColorBrush x:Key="primary-300" Color="#BAECFF" />
    <SolidColorBrush x:Key="accent-100" Color="#FFB400" />
    <SolidColorBrush x:Key="accent-200" Color="#B37E00" />
    <SolidColorBrush x:Key="text-100" Color="#FFFFFF" />
    <SolidColorBrush x:Key="text-200" Color="#cbcbcb" />
    <SolidColorBrush x:Key="bg-100" Color="#007BA7" />
    <SolidColorBrush x:Key="bg-200" Color="#00759F" />
    <SolidColorBrush x:Key="bg-300" Color="#00A0D9" />

    <Color x:Key="primary-1000">#00A8E8</Color>
    <Color x:Key="primary-2000">#0076A2</Color>
    <Color x:Key="primary-3000">#BAECFF</Color>
    <Color x:Key="accent-1000">#FFB400</Color>
    <Color x:Key="accent-2000">#B37E00</Color>
    <Color x:Key="text-1000">#FFFFFF</Color>
    <Color x:Key="text-2000">#cbcbcb</Color>
    <Color x:Key="bg-1000">#007BA7</Color>
    <Color x:Key="bg-2000">#00759F</Color>
    <Color x:Key="bg-3000">#00A0D9</Color>
</ResourceDictionary>