<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <!--
        primary-100:#FFFFFF;
        primary-200:#e0e0e0;
        primary-300:#9b9b9b;
        accent-100:#7F7F7F;
        accent-200:#ffffff;
        text-100:#FFFFFF;
        text-200:#777777;
        bg-100:#000000;
        bg-200:#161616;
        bg-300:#2c2c2c;
    -->

    <SolidColorBrush x:Key="primary-100" Color="#FFFFFF" />
    <SolidColorBrush x:Key="primary-200" Color="#e0e0e0" />
    <SolidColorBrush x:Key="primary-300" Color="#9b9b9b" />
    <SolidColorBrush x:Key="accent-100" Color="#7F7F7F" />
    <SolidColorBrush x:Key="accent-200" Color="#ffffff" />
    <SolidColorBrush x:Key="text-100" Color="#FFFFFF" />
    <SolidColorBrush x:Key="text-200" Color="#777777" />
    <SolidColorBrush x:Key="bg-100" Color="#000000" />
    <SolidColorBrush x:Key="bg-200" Color="#161616" />
    <SolidColorBrush x:Key="bg-300" Color="#2c2c2c" />

    <Color x:Key="primary-1000">#FFFFFF</Color>
    <Color x:Key="primary-2000">#e0e0e0</Color>
    <Color x:Key="primary-3000">#9b9b9b</Color>
    <Color x:Key="accent-1000">#7F7F7F</Color>
    <Color x:Key="accent-2000">#ffffff</Color>
    <Color x:Key="text-1000">#FFFFFF</Color>
    <Color x:Key="text-2000">#777777</Color>
    <Color x:Key="bg-1000">#000000</Color>
    <Color x:Key="bg-2000">#161616</Color>
    <Color x:Key="bg-3000">#2c2c2c</Color>
</ResourceDictionary>