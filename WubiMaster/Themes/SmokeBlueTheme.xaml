<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <!--
        primary-100:#1e295a;
        primary-200:#4c5187;
        primary-300:#abacea;
        accent-100:#F18F01;
        accent-200:#833500;
        text-100:#353535;
        text-200:#5f5f5f;
        bg-100:#F5ECD7;
        bg-200:#ebe2cd;
        bg-300:#c2baa6;
    -->

    <SolidColorBrush x:Key="primary-100" Color="#1e295a" />
    <SolidColorBrush x:Key="primary-200" Color="#4c5187" />
    <SolidColorBrush x:Key="primary-300" Color="#abacea" />
    <SolidColorBrush x:Key="accent-100" Color="#F18F01" />
    <SolidColorBrush x:Key="accent-200" Color="#833500" />
    <SolidColorBrush x:Key="text-100" Color="#353535" />
    <SolidColorBrush x:Key="text-200" Color="#5f5f5f" />
    <SolidColorBrush x:Key="bg-100" Color="#F5ECD7" />
    <SolidColorBrush x:Key="bg-200" Color="#ebe2cd" />
    <SolidColorBrush x:Key="bg-300" Color="#c2baa6" />

    <Color x:Key="primary-1000">#1e295a</Color>
    <Color x:Key="primary-2000">#4c5187</Color>
    <Color x:Key="primary-3000">#abacea</Color>
    <Color x:Key="accent-1000">#F18F01</Color>
    <Color x:Key="accent-2000">#833500</Color>
    <Color x:Key="text-1000">#353535</Color>
    <Color x:Key="text-2000">#5f5f5f</Color>
    <Color x:Key="bg-1000">#F5ECD7</Color>
    <Color x:Key="bg-2000">#ebe2cd</Color>
    <Color x:Key="bg-3000">#c2baa6</Color>
</ResourceDictionary>