<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <!--
        primary-100:#FFD700;
        primary-200:#ddb900;
        primary-300:#917800;
        accent-100:#c49216;
        accent-200:#5e3b00;
        text-100:#dcdcdc;
        text-200:#929292;
        bg-100:#1E1E1E;
        bg-200:#2d2d2d;
        bg-300:#454545;
    -->

    <SolidColorBrush x:Key="BackColor" Color="#25292F" />
    <SolidColorBrush x:Key="BorderColor" Color="#2B2F37" />
    <SolidColorBrush x:Key="PageColor" Color="#2B2F37" />
    <SolidColorBrush x:Key="NavPanelColor" Color="#2B2F37" />
    <SolidColorBrush x:Key="NavHoverColor" Color="#25292F" />
    <SolidColorBrush x:Key="IconColor" Color="#02B1E4" />
    <SolidColorBrush x:Key="TextColor" Color="#FFFFFF" />
    <SolidColorBrush x:Key="TextHoverColor" Color="#FFFFFF" />
    <SolidColorBrush x:Key="BtnRestColor" Color="#02B1E4" />
    <SolidColorBrush x:Key="BtnHoverColor" Color="#FFFFFF" />
    <SolidColorBrush x:Key="BtnPressedColor" Color="White" />

    <!--  ToolButton  -->
    <SolidColorBrush x:Key="ToolButtonColor" Color="#25292F" />
    <SolidColorBrush x:Key="ToolButtonHoverColor" Color="#BCBFC4" />
    <SolidColorBrush x:Key="ToolButtonPressedColor" Color="#7E7E7E" />
