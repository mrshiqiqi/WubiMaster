<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <!--
        primary-100:#FFDAB9;
        primary-200:#dfb28a;
        primary-300:#8c633c;
        accent-100:#ffbda3;
        accent-200:#975f48;
        text-100:#000000;
        text-200:#615353;
        bg-100:#F9AFAF;
        bg-200:#eea5a5;
        bg-300:#c57f80;
    -->

    <SolidColorBrush x:Key="primary-100" Color="#FFDAB9" />
    <SolidColorBrush x:Key="primary-200" Color="#dfb28a" />
    <SolidColorBrush x:Key="primary-300" Color="#8c633c" />
    <SolidColorBrush x:Key="accent-100" Color="#ffbda3" />
    <SolidColorBrush x:Key="accent-200" Color="#975f48" />
    <SolidColorBrush x:Key="text-100" Color="#000000" />
    <SolidColorBrush x:Key="text-200" Color="#615353" />
    <SolidColorBrush x:Key="bg-100" Color="#F9AFAF" />
    <SolidColorBrush x:Key="bg-200" Color="#eea5a5" />
    <SolidColorBrush x:Key="bg-300" Color="#c57f80" />

    <Color x:Key="primary-1000">#FFDAB9</Color>
    <Color x:Key="primary-2000">#dfb28a</Color>
    <Color x:Key="primary-3000">#8c633c</Color>
    <Color x:Key="accent-1000">#ffbda3</Color>
    <Color x:Key="accent-2000">#975f48</Color>
    <Color x:Key="text-1000">#000000</Color>
    <Color x:Key="text-2000">#615353</Color>
    <Color x:Key="bg-1000">#F9AFAF</Color>
    <Color x:Key="bg-2000">#eea5a5</Color>
    <Color x:Key="bg-3000">#c57f80</Color>
</ResourceDictionary>