<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <!--
        primary-100:#2E8B57;
        primary-200:#61bc84;
        primary-300:#c6ffe6;
        accent-100:#8FBC8F;
        accent-200:#345e37;
        text-100:#FFFFFF;
        text-200:#e0e0e0;
        bg-100:#1E1E1E;
        bg-200:#2d2d2d;
        bg-300:#454545;
    -->

    <SolidColorBrush x:Key="primary-100" Color="#2E8B57" />
    <SolidColorBrush x:Key="primary-200" Color="#61bc84" />
    <SolidColorBrush x:Key="primary-300" Color="#c6ffe6" />
    <SolidColorBrush x:Key="accent-100" Color="#8FBC8F" />
    <SolidColorBrush x:Key="accent-200" Color="#345e37" />
    <SolidColorBrush x:Key="text-100" Color="#FFFFFF" />
    <SolidColorBrush x:Key="text-200" Color="#e0e0e0" />
    <SolidColorBrush x:Key="bg-100" Color="#1E1E1E" />
    <SolidColorBrush x:Key="bg-200" Color="#2d2d2d" />
    <SolidColorBrush x:Key="bg-300" Color="#454545" />

    <Color x:Key="primary-1000">#2E8B57</Color>
    <Color x:Key="primary-2000">#61bc84</Color>
    <Color x:Key="primary-3000">#c6ffe6</Color>
    <Color x:Key="accent-1000">#8FBC8F</Color>
    <Color x:Key="accent-2000">#345e37</Color>
    <Color x:Key="text-1000">#FFFFFF</Color>
    <Color x:Key="text-2000">#e0e0e0</Color>
    <Color x:Key="bg-1000">#1E1E1E</Color>
    <Color x:Key="bg-2000">#2d2d2d</Color>
    <Color x:Key="bg-3000">#454545</Color>
</ResourceDictionary>