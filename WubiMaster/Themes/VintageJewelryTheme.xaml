<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <!--
        primary-100:#BFAE9F;
        primary-200:#937962;
        primary-300:#FFFFFF;
        accent-100:#C9BEB9;
        accent-200:#978178;
        text-100:#4D4D4D;
        text-200:#9e9e9e;
        bg-100:#F5EFE8;
        bg-200:#EEE3D7;
        bg-300:#FFFFFF;
    -->

    <SolidColorBrush x:Key="primary-100" Color="#BFAE9F" />
    <SolidColorBrush x:Key="primary-200" Color="#937962" />
    <SolidColorBrush x:Key="primary-300" Color="#FFFFFF" />
    <SolidColorBrush x:Key="accent-100" Color="#C9BEB9" />
    <SolidColorBrush x:Key="accent-200" Color="#978178" />
    <SolidColorBrush x:Key="text-100" Color="#4D4D4D" />
    <SolidColorBrush x:Key="text-200" Color="#9e9e9e" />
    <SolidColorBrush x:Key="bg-100" Color="#F5EFE8" />
    <SolidColorBrush x:Key="bg-200" Color="#EEE3D7" />
    <SolidColorBrush x:Key="bg-300" Color="#FFFFFF" />

    <Color x:Key="primary-1000">#BFAE9F</Color>
    <Color x:Key="primary-2000">#937962</Color>
    <Color x:Key="primary-3000">#FFFFFF</Color>
    <Color x:Key="accent-1000">#C9BEB9</Color>
    <Color x:Key="accent-2000">#978178</Color>
    <Color x:Key="text-1000">#4D4D4D</Color>
    <Color x:Key="text-2000">#9e9e9e</Color>
    <Color x:Key="bg-1000">#F5EFE8</Color>
    <Color x:Key="bg-2000">#EEE3D7</Color>
    <Color x:Key="bg-3000">#FFFFFF</Color>
</ResourceDictionary>