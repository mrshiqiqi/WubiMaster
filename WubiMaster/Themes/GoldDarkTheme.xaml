<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <!--
        primary-100:#FFD700;
        primary-200:#ddb900;
        primary-300:#917800;
        accent-100:#c49216;
        accent-200:#5e3b00;
        text-100:#dcdcdc;
        text-200:#929292;
        bg-100:#1E1E1E;
        bg-200:#2d2d2d;
        bg-300:#454545;
    -->

    <SolidColorBrush x:Key="primary-100" Color="#FFD700" />
    <SolidColorBrush x:Key="primary-200" Color="#ddb900" />
    <SolidColorBrush x:Key="primary-300" Color="#917800" />
    <SolidColorBrush x:Key="accent-100" Color="#c49216" />
    <SolidColorBrush x:Key="accent-200" Color="#5e3b00" />
    <SolidColorBrush x:Key="text-100" Color="#dcdcdc" />
    <SolidColorBrush x:Key="text-200" Color="#929292" />
    <SolidColorBrush x:Key="bg-100" Color="#1E1E1E" />
    <SolidColorBrush x:Key="bg-200" Color="#2d2d2d" />
    <SolidColorBrush x:Key="bg-300" Color="#454545" />

    <Color x:Key="primary-1000">#FFD700</Color>
    <Color x:Key="primary-2000">#ddb900</Color>
    <Color x:Key="primary-3000">#917800</Color>
    <Color x:Key="accent-1000">#c49216</Color>
    <Color x:Key="accent-2000">#5e3b00</Color>
    <Color x:Key="text-1000">#dcdcdc</Color>
    <Color x:Key="text-2000">#929292</Color>
    <Color x:Key="bg-1000">#1E1E1E</Color>
    <Color x:Key="bg-2000">#2d2d2d</Color>
    <Color x:Key="bg-3000">#454545</Color>
</ResourceDictionary>