<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <!--
        primary-100:#6c35de;
        primary-200:#a364ff;
        primary-300:#ffc7ff;
        accent-100:#cb80ff;
        accent-200:#373737;
        text-100:#ffffff;
        text-200:#e0e0e0;
        bg-100:#241b35;
        bg-200:#342a45;
        bg-300:#4d425f;
    -->

    <SolidColorBrush x:Key="primary-100" Color="#6c35de" />
    <SolidColorBrush x:Key="primary-200" Color="#a364ff" />
    <SolidColorBrush x:Key="primary-300" Color="#ffc7ff" />
    <SolidColorBrush x:Key="accent-100" Color="#cb80ff" />
    <SolidColorBrush x:Key="accent-200" Color="#373737" />
    <SolidColorBrush x:Key="text-100" Color="#ffffff" />
    <SolidColorBrush x:Key="text-200" Color="#e0e0e0" />
    <SolidColorBrush x:Key="bg-100" Color="#241b35" />
    <SolidColorBrush x:Key="bg-200" Color="#342a45" />
    <SolidColorBrush x:Key="bg-300" Color="#4d425f" />

    <Color x:Key="primary-1000">#6c35de</Color>
    <Color x:Key="primary-2000">#a364ff</Color>
    <Color x:Key="primary-3000">#ffc7ff</Color>
    <Color x:Key="accent-1000">#cb80ff</Color>
    <Color x:Key="accent-2000">#373737</Color>
    <Color x:Key="text-1000">#ffffff</Color>
    <Color x:Key="text-2000">#e0e0e0</Color>
    <Color x:Key="bg-1000">#241b35</Color>
    <Color x:Key="bg-2000">#342a45</Color>
    <Color x:Key="bg-3000">#4d425f</Color>
</ResourceDictionary>