<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <!--
        primary-100:#FF00FF;
        primary-200:#B300B2;
        primary-300:#FFE5FF;
        accent-100:#00FFFF;
        accent-200:#00B2B3;
        text-100:#cccccc;
        text-200:#7b7b7b;
        bg-100:#141414;
        bg-200:#0E0E0E;
        bg-300:#131313;
    -->

    <SolidColorBrush x:Key="primary-100" Color="#FF00FF" />
    <SolidColorBrush x:Key="primary-200" Color="#B300B2" />
    <SolidColorBrush x:Key="primary-300" Color="#FFE5FF" />
    <SolidColorBrush x:Key="accent-100" Color="#00FFFF" />
    <SolidColorBrush x:Key="accent-200" Color="#00B2B3" />
    <SolidColorBrush x:Key="text-100" Color="#cccccc" />
    <SolidColorBrush x:Key="text-200" Color="#7b7b7b" />
    <SolidColorBrush x:Key="bg-100" Color="#141414" />
    <SolidColorBrush x:Key="bg-200" Color="#0E0E0E" />
    <SolidColorBrush x:Key="bg-300" Color="#131313" />

    <Color x:Key="primary-1000">#FF00FF</Color>
    <Color x:Key="primary-2000">#B300B2</Color>
    <Color x:Key="primary-3000">#FFE5FF</Color>
    <Color x:Key="accent-1000">#00FFFF</Color>
    <Color x:Key="accent-2000">#00B2B3</Color>
    <Color x:Key="text-1000">#cccccc</Color>
    <Color x:Key="text-2000">#7b7b7b</Color>
    <Color x:Key="bg-1000">#141414</Color>
    <Color x:Key="bg-2000">#0E0E0E</Color>
    <Color x:Key="bg-3000">#131313</Color>
</ResourceDictionary>