<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <!--
        primary-100:#FF6600;
        primary-200:#ff983f;
        primary-300:#ffffa1;
        accent-100:#F5F5F5;
        accent-200:#929292;
        text-100:#FFFFFF;
        text-200:#e0e0e0;
        bg-100:#1D1F21;
        bg-200:#2c2e30;
        bg-300:#444648;
    -->

    <SolidColorBrush x:Key="primary-100" Color="#FF6600" />
    <SolidColorBrush x:Key="primary-200" Color="#ff983f" />
    <SolidColorBrush x:Key="primary-300" Color="#ffffa1" />
    <SolidColorBrush x:Key="accent-100" Color="#F5F5F5" />
    <SolidColorBrush x:Key="accent-200" Color="#929292" />
    <SolidColorBrush x:Key="text-100" Color="#FFFFFF" />
    <SolidColorBrush x:Key="text-200" Color="#e0e0e0" />
    <SolidColorBrush x:Key="bg-100" Color="#1D1F21" />
    <SolidColorBrush x:Key="bg-200" Color="#2c2e30" />
    <SolidColorBrush x:Key="bg-300" Color="#444648" />

    <Color x:Key="primary-1000">#FF6600</Color>
    <Color x:Key="primary-2000">#ff983f</Color>
    <Color x:Key="primary-3000">#ffffa1</Color>
    <Color x:Key="accent-1000">#F5F5F5</Color>
    <Color x:Key="accent-2000">#929292</Color>
    <Color x:Key="text-1000">#FFFFFF</Color>
    <Color x:Key="text-2000">#e0e0e0</Color>
    <Color x:Key="bg-1000">#1D1F21</Color>
    <Color x:Key="bg-2000">#2c2e30</Color>
    <Color x:Key="bg-3000">#444648</Color>
</ResourceDictionary>