<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <!--
        primary-100:#4CAF50;
        primary-200:#2a9235;
        primary-300:#0a490a;
        accent-100:#FFC107;
        accent-200:#916400;
        text-100:#333333;
        text-200:#5c5c5c;
        bg-100:#e6fbe3;
        bg-200:#dcf1d9;
        bg-300:#b4c8b1;
    -->

    <SolidColorBrush x:Key="primary-100" Color="#4CAF50" />
    <SolidColorBrush x:Key="primary-200" Color="#2a9235" />
    <SolidColorBrush x:Key="primary-300" Color="#0a490a" />
    <SolidColorBrush x:Key="accent-100" Color="#FFC107" />
    <SolidColorBrush x:Key="accent-200" Color="#916400" />
    <SolidColorBrush x:Key="text-100" Color="#333333" />
    <SolidColorBrush x:Key="text-200" Color="#5c5c5c" />
    <SolidColorBrush x:Key="bg-100" Color="#e6fbe3" />
    <SolidColorBrush x:Key="bg-200" Color="#dcf1d9" />
    <SolidColorBrush x:Key="bg-300" Color="#b4c8b1" />

    <Color x:Key="primary-1000">#4CAF50</Color>
    <Color x:Key="primary-2000">#2a9235</Color>
    <Color x:Key="primary-3000">#0a490a</Color>
    <Color x:Key="accent-1000">#FFC107</Color>
    <Color x:Key="accent-2000">#916400</Color>
    <Color x:Key="text-1000">#333333</Color>
    <Color x:Key="text-2000">#5c5c5c</Color>
    <Color x:Key="bg-1000">#e6fbe3</Color>
    <Color x:Key="bg-2000">#dcf1d9</Color>
    <Color x:Key="bg-3000">#b4c8b1</Color>
</ResourceDictionary>