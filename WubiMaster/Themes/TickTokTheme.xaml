<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <!--
        primary-100:#EE1D52;
        primary-200:#ff607e;
        primary-300:#ffcddf;
        accent-100:#FFC107;
        accent-200:#916400;
        text-100:#333333;
        text-200:#5c5c5c;
        bg-100:#F7F7F7;
        bg-200:#ededed;
        bg-300:#c4c4c4;
    -->

    <SolidColorBrush x:Key="primary-100" Color="#EE1D52" />
    <SolidColorBrush x:Key="primary-200" Color="#ff607e" />
    <SolidColorBrush x:Key="primary-300" Color="#ffcddf" />
    <SolidColorBrush x:Key="accent-100" Color="#FFC107" />
    <SolidColorBrush x:Key="accent-200" Color="#916400" />
    <SolidColorBrush x:Key="text-100" Color="#333333" />
    <SolidColorBrush x:Key="text-200" Color="#5c5c5c" />
    <SolidColorBrush x:Key="bg-100" Color="#F7F7F7" />
    <SolidColorBrush x:Key="bg-200" Color="#ededed" />
    <SolidColorBrush x:Key="bg-300" Color="#c4c4c4" />

    <Color x:Key="primary-1000">#EE1D52</Color>
    <Color x:Key="primary-2000">#ff607e</Color>
    <Color x:Key="primary-3000">#ffcddf</Color>
    <Color x:Key="accent-1000">#FFC107</Color>
    <Color x:Key="accent-2000">#916400</Color>
    <Color x:Key="text-1000">#333333</Color>
    <Color x:Key="text-2000">#5c5c5c</Color>
    <Color x:Key="bg-1000">#F7F7F7</Color>
    <Color x:Key="bg-2000">#ededed</Color>
    <Color x:Key="bg-3000">#c4c4c4</Color>
</ResourceDictionary>