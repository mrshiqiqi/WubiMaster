<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <!--
        primary-100:#bb2649;
        primary-200:#f35d74;
        primary-300:#ffc3d4;
        accent-100:#ffadad;
        accent-200:#ffd6a5;
        text-100:#4b4f5d;
        text-200:#6a738b;
        bg-100:#ffffff;
        bg-200:#f5f5f5;
        bg-300:#cccccc;
    -->

    <SolidColorBrush x:Key="primary-100" Color="#bb2649" />
    <SolidColorBrush x:Key="primary-200" Color="#f35d74" />
    <SolidColorBrush x:Key="primary-300" Color="#ffc3d4" />
    <SolidColorBrush x:Key="accent-100" Color="#ffadad" />
    <SolidColorBrush x:Key="accent-200" Color="#ffd6a5" />
    <SolidColorBrush x:Key="text-100" Color="#4b4f5d" />
    <SolidColorBrush x:Key="text-200" Color="#6a738b" />
    <SolidColorBrush x:Key="bg-100" Color="#ffffff" />
    <SolidColorBrush x:Key="bg-200" Color="#f5f5f5" />
    <SolidColorBrush x:Key="bg-300" Color="#cccccc" />

    <Color x:Key="primary-1000">#bb2649</Color>
    <Color x:Key="primary-2000">#f35d74</Color>
    <Color x:Key="primary-3000">#ffc3d4</Color>
    <Color x:Key="accent-1000">#ffadad</Color>
    <Color x:Key="accent-2000">#ffd6a5</Color>
    <Color x:Key="text-1000">#4b4f5d</Color>
    <Color x:Key="text-2000">#6a738b</Color>
    <Color x:Key="bg-1000">#ffffff</Color>
    <Color x:Key="bg-2000">#f5f5f5</Color>
    <Color x:Key="bg-3000">#cccccc</Color>
</ResourceDictionary>