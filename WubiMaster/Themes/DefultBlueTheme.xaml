<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <!--
        primary-100:#3F51B5;
        primary-200:#757de8;
        primary-300:#dedeff;
        accent-100:#2196F3;
        accent-200:#003f8f;
        text-100:#333333;
        text-200:#5c5c5c;
        bg-100:#FFFFFF;
        bg-200:#f5f5f5;
        bg-300:#cccccc;
    -->

    <SolidColorBrush x:Key="primary-100" Color="#3F51B5" />
    <SolidColorBrush x:Key="primary-200" Color="#757de8" />
    <SolidColorBrush x:Key="primary-300" Color="#dedeff" />
    <SolidColorBrush x:Key="accent-100" Color="#2196F3" />
    <SolidColorBrush x:Key="accent-200" Color="#003f8f" />
    <SolidColorBrush x:Key="text-100" Color="#333333" />
    <SolidColorBrush x:Key="text-200" Color="#5c5c5c" />
    <SolidColorBrush x:Key="bg-100" Color="#FFFFFF" />
    <SolidColorBrush x:Key="bg-200" Color="#f5f5f5" />
    <SolidColorBrush x:Key="bg-300" Color="#cccccc" />

    <Color x:Key="primary-1000">#3F51B5</Color>
    <Color x:Key="primary-2000">#757de8</Color>
    <Color x:Key="primary-3000">#dedeff</Color>
    <Color x:Key="accent-1000">#2196F3</Color>
    <Color x:Key="accent-2000">#003f8f</Color>
    <Color x:Key="text-1000">#333333</Color>
    <Color x:Key="text-2000">#5c5c5c</Color>
    <Color x:Key="bg-1000">#FFFFFF</Color>
    <Color x:Key="bg-2000">#f5f5f5</Color>
    <Color x:Key="bg-3000">#cccccc</Color>
</ResourceDictionary>